{
  "name": "seneca-parambulator",
  "version": "0.3.0",
  "description": "Seneca plugin that provides parambulator message validation.",
  "main": "parambulator.js",
  "license": "MIT",
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Richard Rodger (http://richardrodger.com)"
  ],
  "scripts": {
    "test": "lab -v -P test -L -t 80",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test  -t 80 -r html > docs/coverage.html",
    "annotate": "docco parambulator.js -o docs/annotated"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-parambulator"
  },
  "keywords": [
    "seneca",
    "plugin",
    "message",
    "validate",
    "parambulator"
  ],
  "dependencies": {
    "lodash": "4.15.0",
    "parambulator": "1.5.2"
  },
  "devDependencies": {
    "coveralls": "2.11.x",
    "docco": "0.7.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-standard": "2.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "lab": "11.0.x",
    "pre-commit": "1.1.x",
    "seneca": "plugin",
    "seneca-entity": "1.3.x"
  },
  "files": [
    "README.md",
    "LICENSE",
    "parambulator.js"
  ],
  "pre-commit": [
    "test"
  ]
}
